{
  "locals": {
    "name": "Flipbase",
    "owner": "Ron Jansen <ron@flipbase.com>",
    "description": "Flipbase",
    "path": "https://s3.eu-central-1.amazonaws.com/www.flipbase.com"
  },
  "output": "./www",
  "plugins": [
    "wintersmith-livereload",
    "wintersmith-tag",
    "./plugins/paginator.coffee",
    "./plugins/slugify.coffee"
  ],

  "tag": {
    "template": "blog-tag-index.jade",
    "perPage": 4
  },

  "require": {
    "moment": "moment",
    "_": "underscore"
  },
  "jade": {
    "pretty": false
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  }
}
